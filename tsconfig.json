{
  "include": ["client"],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "allowJs": false,
    "outDir": "./public",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowUnreachableCode": false,
    "skipLibCheck": true
  }
}
